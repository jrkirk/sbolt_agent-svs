sp {location-question*elaborate*copy-down-attrs
   (state <s> ^name location-question
              ^superstate.operator <o>)
   (<o> ^obj <id>)
-->
   (<s> ^obj <id>)
}


sp {found-closest*elaborate*copy-down-attrs
   (state <s> ^name found-closest
              ^superstate.operator <o>)
   (<o> ^prep <prep>
        ^object <id2>
        ^with <id2>
        ^descriptions <d>
        ^object {<id1> <> <id2>})
-->
   (<s> ^descriptions <d>
        ^prep <prep>
        ^obj <id2>
        ^with <id2>
        ^obj <id1>)
}