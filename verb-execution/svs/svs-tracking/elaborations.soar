sp {svs-tracking*elaborate*objects*from-topstate
   (state <s> ^name svs-tracking
              ^superstate.operator <so>)
   (<so> ^object1.id <ida>
         ^object2.id <idb>
         ^prep <prep>)
-->
   (<s> ^a <ida>
        ^b <idb>
        ^result <res>
        ^tracked-relation <tr>
        ^prep <prep>)
}

