sp {svs-tracking*propose*complete-svs-tracking
   (state <s> ^name svs-tracking
              ^a <obja>
              ^b <objb>
              ^result <res>
              ^prep <prep>
             -^tracked-relation.relation.result false
              ^tracked-relation.relation.result true)
-->
   (<s> ^operator <o> + =) # <
   (<o> ^name complete-svs-tracking
        ^category svs
        ^result true)
}

sp {svs-tracking*propose*complete-svs-tracking*failure
   (state <s> ^name svs-tracking
              ^a <obja>
              ^b <objb>
              ^result <res>)
   (<res> ^failure prep-not-found)
-->
   (<s> ^operator <o> +)
   (<o> ^name complete-svs-tracking
        ^category svs
        ^result false)
}

sp {svs-tracking*apply*complete-svs-tracking
   (state <s> ^name svs-tracking
              ^operator <o>
              ^a <obja>
              ^b <objb>
              ^superstate <ss>
              ^result <res>)
   (<o> ^name complete-svs-tracking
        ^result true)
-->
   (<ss> ^track-result success)
}

sp {svs-tracking*apply*complete-svs-tracking*fail
   (state <s> ^name svs-tracking
              ^operator <o>
              ^a <obja>
              ^b <objb>
              ^superstate <ss>
              ^result <res>)
   (<o> ^name complete-svs-tracking
        ^result false)
-->
   (<ss> ^track-result <res>)
}