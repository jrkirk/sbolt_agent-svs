
sp {apply*init-prepositions
   (state <s> ^superstate nil)
-->
   (<s> ^predicates <p>)
}

#to enable integer comparisons
sp {svs*elaborate*predicate*objects*to*int 
   (state <s> ^superstate nil
   	          ^svs.command <cmd>)
   (<cmd> ^extract <E>)
   (<E> ^result.<< positive negative >>.atom <A>)
   (<A> ^a <obja1>
        ^b <objb2>) 
 --> 
   (<A> ^a1 (int <obja1>)
        ^b1 (int <objb2>))
 }

sp {svs*elaborate*svs*objects*ids*to*int 
   (state <s> ^superstate nil
   	          ^svs.spatial-scene <sc>)
   (<sc> ^child <C>)
   (<C> ^id <obja1>) 
 --> 
   (<C> ^id-int (int <obja1>))
 }

sp {clean*up*extracts
   (state <s> ^operator.name pop-segment
              ^superstate nil
              ^svs.command <cmd>)
   (<cmd> ^extract <E11>)
   (<E11> ^a <N7> ^b <N8>)
-->
   (<cmd> ^extract <E11> -)
}
sp {extractsW
   (state <s> ^name << svs-label svs-query >>
              ^topstate.svs.command <cmd>)
-->
   (<cmd> ^extract <E11>)
   (<E11> ^a <N7> ^b <N8> ^type west-of)
   (<N7> ^type all_nodes)
   (<N8> ^type all_nodes)}

