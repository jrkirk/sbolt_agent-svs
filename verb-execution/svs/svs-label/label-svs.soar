sp {svs-label*direct-compare
   (state <s> ^name svs-label
              ^superstate.operator.message.fields.objects <objs>
   			  ^superstate.superstate <sss>
              ^label-svs <l>)
   (<sss> ^svs.command <cmd>)
   (<objs> ^type <type>
           ^obj <oa>
           ^obj <ob>)
   (<ob> ^index <i2> ^id <objb>)
   (<oa> ^index <i1> ^id <obja>)
   (<s> ^predicate <p1>)
   (<p1> ^name <type>
         ^obj <o1>
         ^obj <o2>)
   (<o2> ^index <i2>)
   (<o1> ^index <i1>)
   (<o1> ^index < <i2>)
   (<cmd> ^extract <E>)
   (<E> ^type <t> ^result.positive.atom <A>)
   (<A> ^a1 <obja>
        ^b1 <objb>)   
-->
   (<l> ^result <r>)
   (<r> ^type <t>
        ^a <i1>
        ^b <i2>)
}


sp {svs-label*direct-create*new*prep
   (state <s> ^name svs-label
              ^superstate.operator.message.fields.objects <objs>
   			  ^superstate <ss>
              ^superstate.superstate <sss>
             -^status old
              ^status default)
   (<objs> ^type <type>)
-->
   (<s> ^label-svs <l>)
   (<s> ^created true)
   (<s> ^predicate <p1>)
   (<p1> ^name <type>) 
}

sp {svs-label*direct-add*new*obj*to*prep
   (state <s> ^name svs-label
              ^superstate.operator.message.fields.objects <objs>
   			  ^superstate <ss>
              ^superstate.superstate <sss>
             -^status old
              ^status default)
   (<objs> ^type <type>
           ^obj <o>)
   (<o> ^index <i>)  
   (<s> ^predicate <p1>)
   (<p1> ^name <type>)
-->
   (<p1> ^obj <o1>)
   (<o1> ^index <i>)
}


sp {svs*label*propose*lookup-preposition
   (state <s> ^name svs-label
             -^status
              ^superstate.operator.message.fields.objects <objs>)
   (<objs> ^type <type>)
-->
   (<s> ^operator <o> +)
   (<o> ^name lookup-preposition
        ^category svs
        ^type <type>)
}
