sp {relation-messaging*propose*index-object1
   (state <s> ^topstate <s>
              ^interaction.stack.segment <seg>)
   (<seg> ^purpose <p>
         -^info.obj1)
   (<p> ^type relation-message
        ^parameters.object1 <obj1>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name index-object
        ^use-interaction true
		^allow-ties true
        ^obj 1
        ^category object-processing
	    ^object <obj1>)
}

sp {relation-messaging*propose*index-object2
   (state <s> ^topstate <s>
              ^interaction.stack.segment <seg>)
   (<seg> ^purpose <p>
         -^info.obj2
          ^info.obj1)
   (<p> ^type relation-message
        ^parameters.object2 <obj2>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name index-object
        ^use-interaction true
		^allow-ties true
        ^obj 2
        ^category object-processing
	    ^object <obj2>)
}


