sp {relation-messaging*propose*process-svs-message
   (state <s> #-^processed
               ^interaction.stack.segment <seg>)
   (<seg> ^purpose <p>
          ^info.obj1 <id1>
          ^info.obj2 <id2>)
   (<p> ^type relation-message
        ^parameters.prep <prep>)
-->
   (<s> ^operator <o> +)
   (<o> ^name process-svs-message
        ^category svs
	    ^message <m>)
   (<m> ^fields <f> ^type svs-label)
   (<f> ^objects <object>)
   (<object> ^obj <obja>
             ^obj <objb>
             ^type <prep>)
   (<obja> ^id <id1> ^index 1)
   (<objb> ^id <id2> ^index 2)
} 


