sp {propose*init-simple-agent
   (state <s> ^superstate nil
   	      -^name)
-->
   (<s> ^operator <op> +)
   (<op> ^name init-agent)
}

sp {apply*init-simple-agent
   (state <s> ^operator <o>)
   (<o> ^name init-agent)
-->
   (<s> ^name simple-agent)
}


sp {propose*apply*action
   (state <s> ^name simple-agent
   	      ^io.input-link <il>)
   (<il> ^message <mes>)
-->
   (<s> ^operator <op>
   	^message <mes>)
   (<op> ^name apply-action)
}


sp {elaborate*apply*action*pickup
   (state <s> ^name simple-agent
   	      ^operator <op>
	      ^message <mes>)
   (<op> ^name apply-action)
   (<mes> ^verb-command <vc>)
   (<vc> ^word pick)
   (<vc> ^preposition.word up
   	 ^preposition.object.word cylinder)
-->
   (<op> ^name pick-up
   	 ^object-id 5)
}

sp {apply*action*pick-up
   (state <s> ^name simple-agent
   	      ^operator <op>
	      ^io.output-link <out>)
   (<op> ^name pick-up
   	 ^object-id 5)
-->
   (<out> ^pick-up.object-id 5)
}


sp {elaborate*apply*action*putdown
   (state <s> ^name simple-agent
   	      ^operator <op>
	      ^message <mes>)
   (<op> ^name apply-action)
   (<mes> ^verb-command <vc>)
   (<vc> ^word put)
   (<vc> ^preposition.word down
   	 ^preposition.object.word <any>)
-->
   (<op> ^name put-down)
}

sp {apply*action*put-down
   (state <s> ^name simple-agent
   	      ^operator <op>
	      ^io.output-link <out>)
   (<op> ^name put-down)
-->
   (<out> ^put-down.location current)
}
 
sp {propose*wait
   (state <s> ^attribute state
              ^choices none
             -^operator.name wait)
  	
-->
   (<s> ^operator <o> +)
   (<o> ^name wait)
}

sp {elaborate*apply*action*goto*stove
   (state <s> ^name simple-agent
   	      ^operator <op> 
	      ^message <mes>)
   (<mes> ^verb-command <vc>)
   (<vc> ^word go)
   (<vc> ^preposition.word to
   	 ^preposition.object.word <loc>)
-->
   (<op> ^name goto
   	 ^location <loc>)
}

sp {apply*action*goto*stove
   (state <s> ^name simple-agent
   	      ^operator <op>
	      ^io.output-link <out>)
   (<op> ^name goto
   	 ^location stove)
-->
   (<out> ^goto.location <loc>)
   (<loc> ^x 2.1 ^y 0.90 ^z 0)
 }	      

