sp {find-object*apply*object-match
   (state <s> ^name find-object
              ^operator <o>
             -^return-all true
              ^superstate.operator <ss-o>)
   (<o> ^name object-match
        ^object <obj>)
-->
   (<ss-o> ^match <obj>)
}

sp {find-object*apply*object-match*return-all
   (state <s> ^name find-object
              ^operator.name object-match
              ^operator <o1> +
              ^return-all true
              ^superstate.operator <ss-o>
             -^not-match <obj>)
   (<o1> ^name object-match
         ^object <obj>)
-->
   (<ss-o> ^match <obj>)
}


sp {find-object*add*smem-match
   (state <s> ^name find-object
              ^smem-match {<obj> <> failure}
              ^superstate.operator <ss-o>)
-->
   (<ss-o> ^match <obj>)
}

sp {find-object*propose*no-match
   (state <s> ^name find-object)
   	   #   ^smem-match failure) 
-->
   (<s> ^operator <o> +)
   (<o> ^name no-match
        ^category object-processing)
}

sp {find-object*apply*no-match
   (state <s> ^name find-object
              ^superstate.operator <o>
              ^operator.name no-match)
-->
   (<o> ^match failure)
}