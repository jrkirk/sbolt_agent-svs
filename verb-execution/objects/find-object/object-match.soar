sp {find-object*propose*object-match
   (state <s> ^name find-object
              ^topstate.object <obj>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name object-match
        ^category object-processing
        ^object <obj>)
}

sp {find-object*reject*not-match*category
   (state <s> ^name find-object
              ^operator <o> +
              ^nounjective.category <cat-name>)
   (<o> ^name object-match
        ^object <obj>)
   (<obj> -^property.name <cat-name>)
-->
   (<s> ^operator <o> -
   	^not-match <obj>)
}

sp {find-object*reject*not-match*visual-label
   (state <s> ^name find-object
              ^operator <o> +
              ^nounjective <nj>)
   (<o> ^name object-match
        ^object <obj>)
   (<obj> ^property <prop>)
   (<prop> ^name <cat-name>
           ^type visual
           -^label <label>)
   (<nj> ^category <cat-name>
         ^label <label>)
-->
   (<s> ^operator <o> -
   	^not-match <obj>)
}

sp {find-object*reject*not-match*name
   (state <s> ^name find-object
              ^operator <o> +
              ^nounjective <nj>)
   (<o> ^name object-match
        ^object <obj>)
   (<obj> ^property <prop>)
   (<prop> ^name name
           -^word <name>)
   (<nj> ^category name
         ^word {<name> <> object})
-->
   (<s> ^operator <o> -
   	^not-match <obj>)
}

