# Copy the words from the superstate to create nounjective structures

sp {find-object*elaborate*nounjective
   (state <s> ^name find-object
              ^superstate.operator <o>)
   (<o> ^word <w>)
-->
   (<s> ^nounjective.word <w>)
}

sp {find-object*elaborate*return-all*true
   (state <s> ^name find-object
              ^superstate.operator.return-all true)
-->
   (<s> ^return-all true)
}

sp {find-object*elaborate*return-all*false
   (state <s> ^name find-object
              -^superstate.operator.return-all true)
-->
   (<s> ^return-all false)
}

sp {find-object*elaborate*ask-user*true
   (state <s> ^name find-object
              ^superstate.operator.ask-user true)
-->
   (<s> ^ask-user true)
}

sp {find-object*elaborate*ask-user*false
   (state <s> ^name find-object
              -^superstate.operator.ask-user true)
-->
   (<s> ^ask-user false)
}
