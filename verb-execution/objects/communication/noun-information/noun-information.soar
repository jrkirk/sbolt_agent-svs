# Applying the operator if the substate processing is finished
sp {object-message*specifier*INDEF*apply*noun-information*failure
   (state <s> ^operator <o>
              ^instructor-message <msg>)
   (<o> ^name noun-information
        ^finished failure)
-->
   (<msg> ^purpose <p>)
   (<p> ^type unknown-message
        ^satisfaction.dialog-event unknown-message)
}

# Applying the operator if nothing can be done
sp {object-message*specifier*INDEF*apply*noun-information*failure
   (state <s> ^operator <o>
              ^instructor-message <msg>)
   (<o> ^name noun-information
        ^finished true)
-->
   (<s> ^instructor-message <msg> -)
}