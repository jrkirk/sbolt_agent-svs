### propose goto for all known locations
sp {propose*goto*locations
   (state <s> ^location <loc>
#              ^name eafaefaefa
   	      -^problem-space << lgsoar message-interpretation >>)
-->
   (<s> ^operator <op> +)
   (<op> ^name goto
   	 ^location <loc>)
}

sp {elaborate*goto*location
   (state <s> ^operator <op> +) 
   (<op> ^name goto
   	 ^location <loc>)
   (<loc> ^pose <lp>
   	  ^name <name>)
   (<lp> ^x <x>
   	^y <y> 
	^z <z>)
-->
   (<op> ^name goto
   	 ^location <name>
   	 ^actions.goto.location <loc>
	 ^category primitive-action)
   (<loc> ^x <x> ^y <y> ^z <z>)
}
  
